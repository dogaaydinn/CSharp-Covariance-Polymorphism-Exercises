{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-net/master/src/Stryker.CLI/Stryker.CLI/schema.json",
  "stryker-config": {
    "project": "src/AdvancedConcepts.Core/AdvancedConcepts.Core.csproj",
    "test-projects": [
      "tests/AdvancedConcepts.UnitTests/AdvancedConcepts.UnitTests.csproj"
    ],
    "reporters": [
      "html",
      "progress",
      "cleartext",
      "json"
    ],
    "thresholds": {
      "high": 85,
      "low": 70,
      "break": 65
    },
    "mutation-level": "complete",
    "concurrency": 4,
    "ignore-methods": [
      "*ToString",
      "*GetHashCode",
      "*Equals"
    ],
    "mutate": [
      "!**/Program.cs",
      "!**/Benchmark*.cs"
    ]
  }
}
